{
  "name": "universal-sandbox",
  "version": "4.0.1",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build:tailwindcss": "npx tailwindcss -i resources/css/tailwind.css -o resources/css/output.css",
    "watch:tailwindcss": "npx tailwindcss -i resources/css/tailwind.css -o resources/css/output.css --watch",
    "build:webpack:prod": "npx webpack --config webpack.prod.js",
    "build:webpack:dev": "npx webpack --config webpack.dev.js",
    "watch:webpack:dev": "npx webpack --config webpack.dev.js --watch"
  },
  "dependencies": {
    "autoprefixer": "^10.3.2",
    "axios": "^0.21.1",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.2.0",
    "css-minimizer-webpack-plugin": "^3.0.2",
    "file-loader": "^6.2.0",
    "mini-css-extract-plugin": "^2.2.0",
    "postcss": "^8.3.6",
    "tailwindcss": "^2.2.7",
    "terser-webpack-plugin": "latest",
    "webpack": "^5.51.1",
    "webpack-assets-manifest": "^5.0.6",
    "webpack-cli": "^4.8.0",
    "webpack-manifest-plugin": "^4.0.2",
    "webpack-merge": "^5.8.0"
  }
}
